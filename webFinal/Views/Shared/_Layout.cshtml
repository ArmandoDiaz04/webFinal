<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - webFinal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/webFinal.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/personalizado.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand" style="background-color: #16213E;">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" style="color: #F2F2F2">
                <img src="/src/img/torogoz.png" alt="Logo" width="50" height="50" class="d-inline-block align-text-top">
                torogoz jobs
                </a>
            <div class="navbar-collapse ">
                <ul class="navbar-nav" style="margin-right: auto;">
                    <li class="nav-item">
                        <a class="nav-link" style="color: #87CEEB" href="#">Buscar Trabajo</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: #87CEEB" href="#">Publicar Trabajo</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: #87CEEB" href="#">Recursos</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: #87CEEB" href="#">Valoraciones</a>
                    </li>
                </ul>
                <ul class="navbar-nav mx-3">
                    <li class="nav-item">
                        <a class="nav-link text-light rounded-4 btn btn-primary pl-3 px-3" asp-area="" asp-controller="Login" asp-action="Index">Iniciar sesion</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div class="container-fluid p-0 m-0">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - webFinal - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
